@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style type="text/css">
    #list th {
        background-color: #ccffff;
        color: #000;
    }

    #list td {
        padding: initial;
    }

    #list input {
        border: 0;
        background: transparent;
        width: 100%;
        height: 100%;
    }
</style>
<table class="table_solid" id="list" border="0" cellspacing="0" style="width:100%">
    <tr>
        <th>申请人姓名</th>
        <td><input type="text" /> </td>
        <th>身份证号码</th>
        <td>111</td>
        <th>性别</th>
        <td>111</td>
        <th>民族</th>
        <td>111</td>
    </tr>
    <tr>
        <th>申请人姓名</th>
        <td>111</td>
        <th>身份证号码</th>
        <td>111</td>
        <th>性别</th>
        <td>111</td>
        <th>民族</th>
        <td><button class="bgbutton" id="btnChooseUser">选择审核人</button></td>
    </tr>
</table>
<script type="text/javascript">
    $(function () {
        $("#btnChooseUser").click(function () {
            $.ajax({
                type: "Get",
                url: '@Url.Action("ChooseShUser")',
                dataType: "json",
                success: function (result) {
                    $("#dialog").html(result).dialog({
                        title: "选择审核人",
                        width: 430,
                        maxHeight: 500,
                        resizable: true,
                        modal: true,
                    });
                },
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    alert("获取数据异常：" + errorThrown);
                }
            });
        })
    })
</script>